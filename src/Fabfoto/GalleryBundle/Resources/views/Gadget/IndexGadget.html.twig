{% extends 'FabfotoGalleryBundle::layout.html.twig'%}
{% block titlepage %}Fabfoto-gadget{% endblock titlepage %}
{% block content %}
<div class="list_articles">
    <div class="page-header">
        <h1>{% trans %}gadget{% endtrans %} <small>{% trans %}from.fabbook{% endtrans %}</small></h1>
    </div>
        <div  class="row">
        <div class="span12 columns">

        <div class="hero-unit well-small">
            <h1>{% trans %}train_app.title{% endtrans %}</h1>
            <h2>{% trans %}train_app.subtitle{% endtrans %}</h2>
            <p>{% trans %}train_app.content{% endtrans %}</p>
            <h6>{% trans %}train_app.origine{% endtrans %}</h6>
            <a id="mobApp" class="btn btn-success btn-large" rel="popover" href="{{path('mobile_when_train_app')}}" data-content='<img alt="[QR Code]" src="{{ mopa_barcode_url("qr", url("mobile_when_train_app") ) }}"/>' data-original-title="{% trans %}mobile{% endtrans %}">Mobile version Â»</a>
        </div>
    </div>
</div>
</div>
<script>
    $('#mobApp').popover({
                placement: 'top'

            });
</script>
{% endblock content %}
