{% extends 'FabfotoGalleryBundle::layout.html.twig'%}
{% block titlepage %}Fabfoto-News{% endblock titlepage %}
{% block content %}
<div class="list_articles">
    {% for article in articles %}
    <div class="article">
        <h1>{{article.title}}</h1>
        <h2>{{article.subtitle}}</h2>
        <p>{{article.content|raw}}</p>
        <h3 class="author">{{article.author}}</h3>

    </div>
    {% endfor %}
</div>
{% endblock content %}
