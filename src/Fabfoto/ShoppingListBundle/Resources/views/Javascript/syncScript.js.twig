function synchronizeToServer(object){
    jQuery.ajax({
        type: "POST",
        url:"{{path('items_sync')}}",
        data:object,
        success: function(data) {
            $('.result').html(data);
            console.log('Load was performed.');
        }
    });
    
    refreshData();
}
function refreshData(){
        jQuery.ajax({
        type: "POST",
        url:"{{path('item_refresh')}}",
        success: function(data) {
            $('.result').html(data);
            console.log('Load was performed.');
        }
    });
}    
